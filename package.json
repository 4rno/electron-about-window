{
  "name": "about-window",
  "version": "1.4.0",
  "description": "'About App' window for Electron application",
  "scripts": {
    "dep": "npm install && typings install",
    "build-main": "tsc --sourceMap --target es2015 --module commonjs $(npm prefix)/src/index.ts --outDir .",
    "build-renderer": "tsc --sourceMap --target es2015 --module commonjs $(npm prefix)/src/renderer.ts --outDir .",
    "build": "npm run build-main && npm run build-renderer",
    "tslint": "tslint $(git ls-files | grep -E \"\\.ts$\")",
    "stylelint": "stylelint styles/*.css",
    "lint": "npm run tslint && npm run stylelint",
    "example": "(cd example/ && npm install) && electron $(npm prefix)/example",
    "start": "npm run dep && npm run build && npm run example",
    "prepublish": "npm run lint && npm run build && rm -rf example/node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/electron-about-window.git"
  },
  "keywords": [
    "Electron",
    "electron-component",
    "about",
    "window"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/electron-about-window/issues"
  },
  "homepage": "https://github.com/rhysd/electron-about-window#readme",
  "devDependencies": {
    "@types/electron": "^1.4.25",
    "@types/node": "^6.0.46",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.7"
  },
  "dependencies": {}
}
